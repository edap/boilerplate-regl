{
  "name": "regl-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for regl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "budo --live --open index.js -- -g es2040 -t glslify",
    "build": "browserify index.js -g es2040 -t glslify | uglifyjs -cm| indexhtmlify > index.html"
  },
  "keywords": [
    "regl",
    "boilerplate",
    "budo",
    "browserify"
  ],
  "author": "Davide Prati",
  "license": "ISC",
  "dependencies": {
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "control-panel": "^1.2.0",
    "es2040": "^1.2.6",
    "glslify": "^6.1.0",
    "indexhtmlify": "^1.3.1",
    "regl": "^1.3.0",
    "regl-camera": "^2.1.1",
    "uglify-js": "^3.0.25"
  }
}
